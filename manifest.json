{
  "$schema": "https://json.schemastore.org/chrome-manifest.json",
  "manifest_version": 3,
  "version": "1.0",
  "name": "hoarder-inject",
  "description": "Inject Hoarder bookmarks into your search result",
  "author": "DanSnow",
  "content_scripts": [
    {
      "matches": ["https://www.ecosia.org/search*"],
      "js": ["content.tsx"],
      "css": ["tailwind.css"]
    }
  ]
}
